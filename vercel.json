{
  "version": 2,
  "builds": [
    {
      "src": "api/test_simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/test_simple.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/test_simple.py": {
      "maxDuration": 10
    }
  }
}
